<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marmm.hvaquest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.marmm.hvaquest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.marmm.hvaquest.QuestionActivity" />
        <activity android:name="com.example.marmm.hvaquest.EndActivity" />
        <activity android:name="com.example.marmm.hvaquest.FinishedActivity"></activity>
    </application>

</manifest>